    "axios": "^1.12.2",
    "expo": "~54.0.9",
    "expo-constants": "~18.0.9",
    "expo-dev-client": "~6.0.12",
    "expo-dev-launcher": "^5.0.17",
    "expo-image": "~3.0.8",
